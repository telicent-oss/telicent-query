    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.23.1",
    "tailwindcss": "^3.0.16",
    "web-vitals": "^1.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@eslint/js": "^9.17.0",
